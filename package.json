  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^3.1.8"
  }
}
