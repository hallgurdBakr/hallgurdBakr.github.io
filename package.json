{
  "name": "mysite-11ty",
  "version": "1.0.0",
  "description": "My personal website using Eleventy",
  "main": "index.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve --watch & npx tailwindcss -i ./src/input.css -o ./_site/assets/css/style.css --watch",
